{{view "detail/header" type="detail-header"}}
<div class="content" data-transition-in="{{transitionIn}}" data-transition-out="{{transitionOut}}">
	<div class="content-padded">
	  <h4>{{title}}</h4>

	  <h6>{{#visible}} Visible: {{id}} {{/visible}}</h6>
	  <p>Item {{id}} has a visible status of {{visible}}</p>
	  <p>{{description}}</p>
	  <br>
	  <small>{{extra}}</small>
	</div>
</div>
{{view "detail/footer" type="detail-footer"}}