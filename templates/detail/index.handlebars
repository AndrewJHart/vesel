<div class="contentMask"></div>
{{#view "detail/header" tag="header" className="bar bar-nav" type="detail-header"}}
  	{{#link "" expand-tokens=true class="icon icon-left-nav pull-left"}}{{/link}}
  	<h1 class="title">Details</h1>
{{/view}}
<div class="content">
	<div class="{{category.name}}__bg {{category.name}}-d__bg">
    <div class="detail-cat__icon">
      <span class="{{category.name}}"></span>
    </div>
    <div class="detail-cat__subject">
        <h4>{{subject}}</h4>
    </div>
	</div>
  <ul class="table-view table-view_detail__push">
    <li class="table-view-cell">
        <div id="mapmain" class="map map-detail"></div>
    </li>
    <li class="table-view-cell table-view_detail__push">
        <p>{{information}}</p>
    </li>
  </ul>
</div>
{{#view "detail/footer" tag="nav" className="bar bar-tab" type="detail-footer"}}
  <a class="tab-item active toggle-share">
    <span class="icon icon-share"></span>
    <span class="tab-label">Share This</span>
  </a>
{{/view}}