<li id="{{id}}" class="">
	<a href="#detail/{{id}}" class="navigate-right"></a>
	<div class="topcoat-grid__row">
		<div class="col-25 col-center {{category.name}}__bg ">
			<div class="li-cat__container">
				<span class="{{category.name}} cat"></span>
				<h2 class="li-cat__title">{{category.name}}</h2>
			</div>
		</div>
		<div class="col-75">
			<p class="li-summary"> {{ subject }}</p>
			<div class="li-date__container">
				<p class="li-date">Posted: May 8th, 4:00PM</p>
			</div>
		</div>
	</div>
</li>